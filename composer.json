{
    "name": "larabizcms/larabiz",
    "type": "project",
    "description": "LaraBiz is an Laravel CMS developed based on Laravel Framework 11 and web platform whose sole purpose is to make your development workflow simple again.",
    "keywords": [
        "framework",
        "laravel",
        "cms",
        "larabiz",
        "larabizcms",
        "larabiz cms",
        "larabiz cms laravel",
        "laravel cms",
        "laravel 11 cms"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-simplexml": "*",
        "ext-xmlwriter": "*",
        "ext-zip": "*",
        "guzzlehttp/guzzle": "^7.8",
        "larabizcms/core": "dev-master",
        "laravel/framework": "^11.9",
        "laravel/tinker": "^2.9"
    },
    "require-dev": {
        "larabizcms/for-dev": "^1.0",
        "roave/security-advisories": "dev-latest",
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "merge-plugin": {
            "include": [
                "modules/*/composer.json"
            ]
        },
        "installer-types": [
            "larabiz-module",
            "larabiz-theme"
        ],
        "installer-paths": {
            "modules/{$name}/": [
                "type:larabiz-module"
            ],
            "themes/{$name}/": [
                "type:larabiz-theme"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true,
            "wikimedia/composer-merge-plugin": true,
            "composer/installers": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.larabiz.com"
        }
    ]
}
